<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybzn.mapper.IssueMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ybzn.pojo.Issue">
        <id column="issue_id" property="issueId" />
        <result column="issue_conten" property="issueConten" />
        <result column="is_fill" property="isFill" />
        <result column="issue_type" property="issueType" />
        <result column="issue_options" property="issueOptions" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        issue_id, issue_conten, is_fill, issue_type, issue_options
    </sql>

</mapper>
